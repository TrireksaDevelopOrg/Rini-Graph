<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Aplikasi Penjadwalan | </title>

    <link href="~/bower_components/pnotify/dist/pnotify.css" rel="stylesheet" />
    <link href="~/bower_components/font-awesome/css/font-awesome.css" rel="stylesheet" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- Bootstrap -->
    <!-- Font Awesome -->

    
</head>
<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                <div class="left_col scroll-view">
                    <div class="navbar nav_title" style="border: 0;">
                        <a href="index.html" class="site_title"><i class="glyphicon glyphicon-calendar"></i> <span>Penjadwalan !</span></a>
                    </div>
                    <div class="clearfix"></div>
                    <div class="profile clearfix">
                        <div class="profile_pic">
                            <span>Welcome,</span>
                        </div>
                    </div>
                    <br />
                    <div id="sidebar-menu" class="main_menu_side hidden-print main_menu">
                        <div class="menu_section">
                            <center>
                                <img style="width:60px" src="~/images/Logo_Lantamal_X_Armatim.png" />
                            </center>
                          
                            <ul class="nav side-menu">
                                <li>
                                    <a href="#Home"><i class="fa fa-home"></i> Home <span class="fa fa-chevron-down"></span></a>
                                </li>
                                <li>
                                    <a href="#!perawat"><i class="fa fa-users"></i> Perawat<span class="fa fa-chevron-down"></span></a>
                                </li>



                                <li>
                                    <a href="#!graph"><i class="fa fa-bar-chart-o"></i> Graph<span class="fa fa-chevron-down"></span></a>

                                </li>

                                <li>
                                    <a href="#!jadwal"><i class="fa fa-table"></i> Jadwal <span class="fa fa-chevron-down"></span></a>
                                </li>


                            </ul>
                        </div>

                    </div>
                    <!-- /sidebar menu -->
                    <!-- /menu footer buttons -->
                    <div class="sidebar-footer hidden-small">

                    </div>
                    <!-- /menu footer buttons -->
                </div>
            </div>
            <!-- top navigation -->
            <div class="top_nav">
                <div class="nav_menu">
                    <nav>
                        <div class="nav toggle">
                            <a id="menu_toggle"><i class="fa fa-bars"></i></a>
                        </div>
                        @Html.Partial("_LoginPartial")
                       
                    </nav>
                </div>
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <!-- top tiles -->
                @RenderBody()
            </div>
            <!-- /page content -->
            <!-- footer content -->
            <footer>
                <div class="pull-right">
                 
                </div>
                <div class="clearfix"></div>
            </footer>
            <!-- /footer content -->
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/bower_components/pnotify/dist/pnotify.js"></script>
    <link href="~/bower_components/pnotify/dist/pnotify.buttons.css" rel="stylesheet" />
    <script src="~/bower_components/pnotify/dist/pnotify.buttons.js"></script>
    <script src="~/bower_components/pnotify/dist/pnotify.confirm.js"></script>
    <!-- jQuery -->
</body>
</html>
